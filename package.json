		"typescript": "^5.4.2"
	},
	"dependencies": {
		"@fnlb-project/shared": "https://github.com/FNLB-Project/Pragmatic.git#stable",
		"mongoose": "^8.2.1"
	},
	"trustedDependencies": ["@fnlb-project/shared"]
}
